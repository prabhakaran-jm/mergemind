# Performance optimization gitignore
# Never commit these files to version control

# Performance logs
*.log
logs/
performance/logs/

# Performance reports
reports/
performance/reports/
*_report.json
*_report_*.json

# Performance optimizations
optimizations/
performance/optimizations/
*_optimization.json
*_optimization_*.json

# Performance tests
tests/
performance/tests/
*_test.json
*_test_*.json

# Cache data
cache/
*.cache
.cache/

# Redis data
dump.rdb
*.rdb

# Performance monitoring data
monitoring/
performance/monitoring/
*.monitor
*.metrics

# Temporary files
*.tmp
*.temp
*.bak
*.backup

# OS files
.DS_Store
Thumbs.db

# IDE files
.vscode/
.idea/
*.swp
*.swo

# Python cache
__pycache__/
*.pyc
*.pyo
*.pyd
.Python
build/
develop-eggs/
dist/
downloads/
eggs/
.eggs/
lib/
lib64/
parts/
sdist/
var/
wheels/
*.egg-info/
.installed.cfg
*.egg

# Environment files
.env
.env.local
.env.production
.env.staging
.env.development

# Configuration files with secrets
*_secret.yml
*_secret.yaml
*_secret.json
*_secret.toml

# Performance data
*.perf
*.profile
*.benchmark
