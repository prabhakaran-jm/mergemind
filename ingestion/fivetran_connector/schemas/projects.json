{
  "projects": {
    "description": "GitLab projects and repositories",
    "columns": {
      "project_id": {
        "type": "integer",
        "description": "Unique project ID",
        "primary_key": true
      },
      "name": {
        "type": "string",
        "description": "Project name"
      },
      "path": {
        "type": "string",
        "description": "Project path"
      },
      "path_with_namespace": {
        "type": "string",
        "description": "Full project path with namespace"
      },
      "description": {
        "type": "string",
        "description": "Project description"
      },
      "created_at": {
        "type": "timestamp",
        "description": "When the project was created"
      },
      "last_activity_at": {
        "type": "timestamp",
        "description": "Last activity timestamp"
      },
      "visibility": {
        "type": "string",
        "description": "Project visibility: private, internal, public"
      },
      "web_url": {
        "type": "string",
        "description": "GitLab web URL for the project"
      },
      "default_branch": {
        "type": "string",
        "description": "Default branch name"
      },
      "archived": {
        "type": "boolean",
        "description": "Whether the project is archived"
      },
      "forked_from_project": {
        "type": "integer",
        "description": "ID of the project this was forked from"
      }
    }
  }
}
