# GitLab Configuration for Demo Population
# Copy this file to config.env and update with your values

# GitLab Instance URL (replace with your actual GitLab URL)
GITLAB_BASE_URL=https://your-gitlab-domain.com

# GitLab Personal Access Token (create via GitLab web interface)
GITLAB_TOKEN=your_gitlab_token_here

# Project IDs to populate (comma-separated)
GITLAB_PROJECT_IDS=4,5,6
